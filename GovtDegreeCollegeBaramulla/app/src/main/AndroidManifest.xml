<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.college.baramulla.govtdegreecollegebaramulla">

    <application
        android:name=".GovtDegreeCollegeBaramulla"
        android:allowBackup="true"
        android:icon="@drawable/logo_gdc"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_gdc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Journal_Activity"
            android:screenOrientation="portrait"
            ></activity>
        <activity android:name=".admins_Activity"

            android:screenOrientation="portrait"/>
        <activity android:name=".webViewActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".show_journal_post_activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".Timetable_Activity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            />
        </activity>
        <activity android:name=".semester_activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Display_Timetable_of_Semester"
            android:screenOrientation="portrait"/>
        <activity android:name=".Notifications_Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".register_grievance"
            android:screenOrientation="portrait"/>
    </application>





</manifest>