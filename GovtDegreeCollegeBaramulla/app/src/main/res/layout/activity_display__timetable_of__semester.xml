<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/BgColor"
    android:id="@+id/parent_layout_in_display_timetable_of_sem"
    tools:context="com.college.baramulla.govtdegreecollegebaramulla.Display_Timetable_of_Semester">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/BgColor"
    >
    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar_in_displat_timetableof_semester_activity"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/topcolor"
        android:minHeight="?attr/actionBarSize"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:padding="3dp"
            >
            <ImageButton
                android:id="@+id/imgback_in_display_timetable_of_semester_toolbar"
                android:layout_width="25dp"
                android:layout_height="25dp"
                app:srcCompat="@drawable/ic_chevron_left_black_24dp"
                android:background="@drawable/circle_bg"
                tools:ignore="VectorDrawableCompat"
                android:focusable="true"
                android:clickable="true"/>
        </LinearLayout>

        <TextView
            android:id="@+id/tv_in_displayTimetable_semester_toolbar"
            android:layout_marginLeft="30dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/"
            android:text="[Stream Name]"
            android:textColor="@color/color1"
            android:textSize="18sp"
            />
    </android.support.v7.widget.Toolbar>

    <ListView
        android:id="@+id/listview_display_timetabele"

        android:visibility="visible"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="1dp"
        android:divider="@android:color/transparent"
        android:background="@color/BgColor"

        />

</LinearLayout>
</FrameLayout>
