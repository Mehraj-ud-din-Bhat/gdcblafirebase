<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admins_Activity"
    android:background="@color/BgColor"
    android:orientation="vertical"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/topcolor"
        android:padding="20dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageButton
                android:id="@+id/imgback_in_admins_ac"
                android:layout_width="25dp"
                android:layout_height="25dp"
                app:srcCompat="@drawable/ic_chevron_left_black_24dp"
                android:background="@drawable/circle_bg"
                tools:ignore="VectorDrawableCompat"
                android:focusable="true"
                android:clickable="true"/>
            <TextView
                android:id="@+id/tvDialogactionName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat"
                android:layout_marginTop="1.3dp"
                android:gravity="center"
                android:text="Application Admins"
                android:textColor="@color/color1"
                android:textSize="15sp"

                />

            <ProgressBar
                android:id="@+id/pgbar_in_admin_ac"
                style="?android:attr/progressBarStyle"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:indeterminateTint="@color/white_color"

                />
        </RelativeLayout>

    </LinearLayout>

    <ListView
        android:layout_margin="1dp"
        android:visibility="visible"
        android:id="@+id/listview_show_admins"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="@android:color/transparent"

        />

</LinearLayout>