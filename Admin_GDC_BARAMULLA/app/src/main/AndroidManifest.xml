<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lovingkashmir.admin_gdc_baramulla">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_gdc"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_gdc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"
                    android:screenOrientation="portrait" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".show_journal_post_activity"
            android:screenOrientation="portrait" />
        <activity android:name=".Journal_Activity"
            android:screenOrientation="portrait" />
        <activity android:name=".Notifications_Activity"
            android:screenOrientation="portrait" />
        <activity android:name=".Admin_List_Activity"
            android:screenOrientation="portrait" />
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".Timetable_Activity"
            android:screenOrientation="portrait" >

        </activity>
        <activity android:name=".semester_activity"
            android:screenOrientation="portrait" />
        <activity android:name=".Display_Timetable_of_Semester"
            android:screenOrientation="portrait" />
    </application>

</manifest>